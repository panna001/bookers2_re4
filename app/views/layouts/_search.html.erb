<% if user_signed_in? %>
  <div class="text-center">
    <%= form_with scope: :search, url: search_path, method: :get, local: true do |f| %>
      <%= f.text_field :key_word %>
      <%= f.select :model, ["User", "Book"], value: params[:search][:model] %>
      <%= f.select :how, ["完全一致","前方一致","後方一致","部分一致"] %>
      <%= f.submit "検索" %>
    <% end %>
  </div>
<% end %>